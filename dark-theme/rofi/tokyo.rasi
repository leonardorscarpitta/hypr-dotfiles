* {
  /* Backgrounds */
  bg0 : #0b0b0b;   /* fundo principal */
  bg1 : #111111;   /* input */
  bg2 : #1a1a1a;   /* seleção */
  bg3 : #222222;   /* bordas */

  /* Foregrounds */
  fg0 : #e5e5e5;   /* texto principal */
  fg1 : #b5b5b5;   /* texto secundário */
  fg2 : #777777;   /* texto desabilitado */

  /* Accent minimal */
  accent : #ffffff;
  urgent : #aaaaaa;
}

